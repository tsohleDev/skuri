<div id="<%= dom_id order %>">
  <p>
    <strong>Customer:</strong>
    <%= order.customer_id %>
  </p>

  <p>
    <strong>Cost:</strong>
    <%= order.cost %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= order.status %>
  </p>

  <p>
    <strong>Address1:</strong>
    <%= order.address1 %>
  </p>

  <p>
    <strong>Address2:</strong>
    <%= order.address2 %>
  </p>

  <p>
    <strong>City:</strong>
    <%= order.city %>
  </p>

  <p>
    <strong>State:</strong>
    <%= order.state %>
  </p>

  <p>
    <strong>Postal code:</strong>
    <%= order.postal_code %>
  </p>

</div>
